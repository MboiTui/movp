profiles {

    test {
        params {
            samples="${projectDir}/test/test.csv"
            outdir="${launchDir}/testout"
        }
    }

    standard {
        process.executor = 'local'
        process.container = 'iracooke/movp'
    }

    pbspro {
        process.executor = 'pbspro'
        singularity.enabled = true
        runOptions = "-B ${PWD}:${PWD}"
    }


    genomics12 {
        process.executor = 'local'
        singularity.enabled = true
        runOptions = "-B ${PWD}:${PWD}"
    }

}

